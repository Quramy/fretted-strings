{
  "name": "fretted-strings",
  "version": "2.0.0",
  "description": "Mark on your strings and get it's position",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky",
    "clean": "rimraf -g dist",
    "build": "tsup",
    "lint": "npm run format:check && tsc",
    "prettier": "prettier .",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quramy/fretted-strings.git"
  },
  "keywords": [
    "string"
  ],
  "author": "yosuke.kurami@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quramy/fretted-strings/issues"
  },
  "homepage": "https://github.com/Quramy/fretted-strings#readme",
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "20.17.10",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "prettier": "3.4.2",
    "pretty-quick": "4.0.0",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.5",
    "tsup": "8.3.5",
    "typescript": "5.7.2"
  }
}
